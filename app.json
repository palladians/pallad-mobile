{
	"expo": {
		"name": "Pallad",
		"slug": "pallad",
		"version": "0.0.1",
		"orientation": "portrait",
		"icon": "./src/assets/icon.png",
		"scheme": "pallad",
		"userInterfaceStyle": "automatic",
		"splash": {
			"image": "./src/assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "xyz.palladians.pallad"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./src/assets/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			},
			"package": "xyz.palladians.pallad"
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./src/assets/favicon.png"
		},
		"plugins": [
			"expo-router",
			[
				"expo-font",
				{
					"fonts": ["./src/assets/DMSans.ttf"]
				}
			],
			[
				"react-native-ble-plx",
				{
					"isBackgroundEnabled": true
				}
			],
			[
				"expo-secure-store",
				{
					"faceIDPermission": "Allow Pallad to access your Face ID biometric data."
				}
			]
		],
		"experiments": {
			"typedRoutes": true
		},
		"extra": {
			"router": {
				"origin": false
			},
			"eas": {
				"projectId": "148098d1-7e19-4326-9814-dd661a192776"
			}
		},
		"runtimeVersion": {
			"policy": "appVersion"
		},
		"updates": {
			"url": "https://u.expo.dev/148098d1-7e19-4326-9814-dd661a192776"
		},
		"owner": "gluestack"
	}
}
